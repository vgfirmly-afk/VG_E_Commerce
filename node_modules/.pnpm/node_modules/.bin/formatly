#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/formatly@0.3.0/node_modules/formatly/bin/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/formatly@0.3.0/node_modules/formatly/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/formatly@0.3.0/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/formatly@0.3.0/node_modules/formatly/bin/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/formatly@0.3.0/node_modules/formatly/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/formatly@0.3.0/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../formatly/bin/index.mjs" "$@"
else
  exec node  "$basedir/../formatly/bin/index.mjs" "$@"
fi
