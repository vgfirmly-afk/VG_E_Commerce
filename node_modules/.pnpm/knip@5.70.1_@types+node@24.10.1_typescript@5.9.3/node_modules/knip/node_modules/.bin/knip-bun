#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/knip@5.70.1_@types+node@24.10.1_typescript@5.9.3/node_modules/knip/bin/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/knip@5.70.1_@types+node@24.10.1_typescript@5.9.3/node_modules/knip/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/knip@5.70.1_@types+node@24.10.1_typescript@5.9.3/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/knip@5.70.1_@types+node@24.10.1_typescript@5.9.3/node_modules/knip/bin/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/knip@5.70.1_@types+node@24.10.1_typescript@5.9.3/node_modules/knip/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/knip@5.70.1_@types+node@24.10.1_typescript@5.9.3/node_modules:/Users/vg/Projects/Week_2/01_Main_project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/bun" ]; then
  exec "$basedir/bun"  "$basedir/../../bin/knip-bun.js" "$@"
else
  exec bun  "$basedir/../../bin/knip-bun.js" "$@"
fi
